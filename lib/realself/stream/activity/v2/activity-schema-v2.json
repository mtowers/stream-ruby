{
  "id" : "http://realself.com/activity-schema-v2#",
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "description" : "Activity schema for activity stream items",
  "type" : "object",
  "required" : ["published", "title", "actor", "verb", "object"],
  "properties" : {
    "published" : {
      "$ref": "../shared-schema.json#/definitions/ISO8601"
    },
    "title" : {
    	"type" : "string"
    },
    "actor" : {
        "$ref" : "../shared-schema.json#/definitions/activity-object"
    },
    "verb" : {
        "type" :"string"
    },
    "object" : {
        "$ref" : "../shared-schema.json#/definitions/activity-object"
    },
    "target" : {
        "$ref" : "../shared-schema.json#/definitions/activity-object"
    },
    "extensions" : {
        "type" : "object",
        "minItems" : 0,
        "uniqueItems" : true,
        "additionalProperties" : {"$ref" : "../shared-schema.json#/definitions/activity-object"}
    },
    "uuid" : {
      "$ref": "../shared-schema.json#/definitions/uuid"
    },
    "prototype" : {
      "type" : "string"
    }
  }
}
